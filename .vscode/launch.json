{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "C++ codelldb",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/bin/cpp/${fileBasenameNoExtension}",
      "args": [],
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "C++ build"
    },
    {
      "name": "C++",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/bin/cpp/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": true,
      "preLaunchTask": "C++ build",
      "launchCompleteCommand": "exec-run",
      "osx": {
        "MIMode": "lldb",
        "miDebuggerPath": "/Applications/Xcode.app/Contents/Developer/usr/bin/lldb-mi"
      },
      "linux": {
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true,
          }
        ]
      },
      "windows": {
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    },
    {
      "type": "java",
      "name": "Java",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "javascript",
      "program": "${file}",
      "cwd": "${workspaceRoot}/javascript",
      "env": {}
    }
  ]
}